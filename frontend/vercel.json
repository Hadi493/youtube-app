{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://youtube-backend-latest.onrender.com:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }
  